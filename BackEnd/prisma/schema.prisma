datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
  relationMode = "prisma"
}
generator client {
  provider = "prisma-client-js"
}

model User {
  id       String    @id @default(uuid()) @map("_id")
  username String
  mail     String
  password String
  images   Image[]
}

model Image {
  id         String   @id @default(auto()) //@test.IntegrationTest.Int
  filename   String
  image_type String
  userId     String
  user       User     @relation(fields: [userId], references: [id])
}