generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
  relationMode = "prisma"
}

model User {
  id       Int      @id @default(autoincrement())
  mail     String   @unique
  password String
  username String?
}

model Image {
  id         Int      @id @default(autoincrement())
  image_id   String   @unique
  image_type String
  url        image[]
  userId     Int
  user       User     @relation(fields: [userId], references: [id])
}

